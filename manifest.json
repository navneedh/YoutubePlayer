{
  "manifest_version": 2,

  "name": "UTube",
  "description": "This extension creates a floating Youtube Video in your working tab",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "launch.png",
    "default_title": "UTube"

  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
